<odoo>
    <data>
      <!-- GeoEngine views -->
      <record id="ir_ui_view_poteaugeoview0" model="ir.ui.view">
        <field name="name">signalisation.poteau.geo_view</field>
        <field name="arch" type="xml">
          <geoengine>
            <field name="name"/>
            <field name="info"/>
          </geoengine>
        </field>
        <field eval="16" name="priority"/>
        <field name="model">signalisation.poteau</field>
      </record>

      <record id="geoengine_vector_layer_basictest0" model="geoengine.vector.layer">
        <field name="geo_field_id" ref="signalisation.field_signalisation_poteau_geom"/>
        <field name="name">Basic Poteau</field>
        <field name="view_id" ref="ir_ui_view_poteaugeoview0"/>
        <field name="geo_repr">basic</field>
        <field name="attribute_field_id" ref="field_signalisation_poteau_name"/>
        <field name="begin_color">#FF680A</field>
      </record>

      <record id="geoengine_raster_layer_test0" model="geoengine.raster.layer">
        <field name="raster_type">osm</field>
        <field name="name">ZIP OSM</field>
        <field name="view_id" ref="ir_ui_view_poteaugeoview0"/>
        <field eval="0" name="overlay"/>
      </record>

      <record id="geoengine_raster_layer_osm0" model="geoengine.raster.layer">
        <field name="raster_type">osm</field>
        <field name="name">ZIP OSM Light</field>
        <field name="view_id" ref="ir_ui_view_poteaugeoview0"/>
        <field eval="1" name="overlay"/>
      </record>

      <record id="geoengine_raster_layer_basic0" model="geoengine.raster.layer">
          <field name="raster_type">d_wms</field>
          <field name="name">basic</field>
          <field name="url">vmap0.tiles.osgeo.org/wms/vmap0</field>
          <field name="view_id" ref="ir_ui_view_poteaugeoview0"/>
          <field eval="1" name="overlay"/>
      </record>

      <record id="ir_ui_view_poteaugeoview_lb72" model="ir.ui.view">
        <field name="name">signalisation.poteau.geo_view_lb72</field>
        <field name="arch" type="xml">
          <geoengine>
            <field name="name"/>
            <field name="info"/>
          </geoengine>
        </field>
        <field eval="16" name="priority"/>
        <field name="model">signalisation.poteau</field>
        <field name="projection">EPSG:31370</field>
        <field name="default_extent">0, 0, 300000, 300000</field>
      </record>

      <record id="geoengine_vector_layer_72" model="geoengine.vector.layer">
        <field name="geo_field_id" ref="signalisation.field_signalisation_poteau_geom"/>
        <field name="name">Poteau</field>
        <field name="view_id" ref="ir_ui_view_poteaugeoview_lb72"/>
        <field name="geo_repr">basic</field>
        <field name="attribute_field_id" ref="field_signalisation_poteau_name"/>
        <field name="begin_color">#FF680A</field>
      </record>

      <record id="geoengine_raster_layer_lb72" model="geoengine.raster.layer">
          <field name="raster_type">wmts_cap</field>
          <field name="name">urbisFR</field>
          <field name="url">http://geoservices-urbis.irisnet.be/geoserver/gwc/service/wmts?REQUEST=GetCapabilities</field>
          <field name="view_id" ref="ir_ui_view_poteaugeoview_lb72"/>
          <field name="projection">EPSG:31370</field>
          <field name="matrix_set">EPSG:31370</field>
          <field eval="0" name="overlay"/>
      </record>

      <record id="geoengine_raster_wmslayer_lb72" model="geoengine.raster.layer">
          <field name="raster_type">d_wms</field>
          <field name="name">urbisNL</field>
          <field name="url">http://geoservices-urbis.irisnet.be/geoserver/ows</field>
          <field name="view_id" ref="ir_ui_view_poteaugeoview_lb72"/>
          <field name="projection">EPSG:31370</field>
          <field eval="1" name="overlay"/>
      </record>

    </data>
</odoo>
